{
  "level": "INFO",
  "log_to_file": true,
  "log_to_console": true,
  "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
  "date_format": "%Y-%m-%d %H:%M:%S",
  "max_file_size_mb": 10,
  "backup_count": 5,
  "log_filename_prefix": "leap",
  "rotation_type": "size",
  "rotation_when": "midnight",
  "rotation_interval": 1
}
